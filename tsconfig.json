{
  "extends": "astro/tsconfigs/strict",

  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "noErrorTruncation": true,
    "types": ["astro/client", "@cloudflare/workers-types"],
    "paths": {
      "@layouts/*": ["src/layouts/*"]
    }
  }
}
